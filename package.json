{
  "name": "gyro",
  "description": "命令号工具",
  "version": "0.0.1",
  "author": "LimMem @LimMem",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "build": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "bin": {
    "gyro": "./bin/run"
  },
  "bugs": "https://github.com/LimMem/gyro/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "chalk": "^4.1.2",
    "cli-clear": "^1.0.4",
    "cli-spinner": "^0.2.10",
    "commander": "^8.3.0",
    "download-git-repo": "^3.0.2",
    "execa": "^5.1.1",
    "figlet": "^1.5.2",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "inquirer": "^8.2.0",
    "loading-cli": "^1.1.0",
    "ora": "^6.0.1",
    "pacote": "^12.0.2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^10",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/LimMem/gyro",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "gyro",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "LimMem/gyro",
  "types": "lib/index.d.ts"
}
